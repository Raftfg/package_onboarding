{
    "name": "raftfg/package_onboarding",
    "description": "Package Laravel Onboarding Stateless (Fresh Build)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "onboarding",
        "package"
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Raftfg\\OnboardingPackage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Raftfg\\OnboardingPackage\\OnboardingServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}